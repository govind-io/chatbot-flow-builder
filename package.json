{
  "name": "chatbot-flow-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .prettierignore",
